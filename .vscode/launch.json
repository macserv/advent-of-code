{
    "inputs": [
        { "id": "input-file",                           "type": "pickString", "description": "Select an input file:",                               "options": ["Sample.txt", "Input.txt"],                     "default": "Sample.txt" },
        { "id": "calorie-counting-mode",                "type": "pickString", "description": "Mode argument for calorie-counting:",                 "options": ["1", "3"],                                      "default": "3" },
        { "id": "rock-paper-scissors-mode",             "type": "pickString", "description": "Mode argument for rock-paper-scissors:",              "options": ["hand", "outcome"],                             "default": "outcome" },
        { "id": "rucksack-reorganization-mode",         "type": "pickString", "description": "Mode argument for rucksack-reorganization:",          "options": ["duplicateItems", "teamBadgeItems"],            "default": "teamBadgeItems" },
        { "id": "camp-cleanup-mode",                    "type": "pickString", "description": "Mode argument for camp-cleanup:",                     "options": ["partial", "complete"],                         "default": "complete" },
        { "id": "supply-stacks-mode",                   "type": "pickString", "description": "Mode argument for supply-stacks:",                    "options": ["CM9000", "CM9001"],                            "default": "CM9001" },
        { "id": "tuning-trouble-mode",                  "type": "pickString", "description": "Mode argument for tuning-trouble:",                   "options": ["packet", "message"],                           "default": "message" },
        { "id": "no-space-left-on-device-mode",         "type": "pickString", "description": "Mode argument for no-space-left-on-device:",          "options": ["sumOf100kOrLess", "sizeOfSmallestBigEnough"],  "default": "sizeOfSmallestBigEnough" },
        { "id": "treetop-tree-house-mode",              "type": "pickString", "description": "Mode argument for treetop-tree-house:",               "options": ["visibleTreeCount", "bestScenicScore"],         "default": "bestScenicScore" },
        { "id": "trebuchet-mode",                       "type": "pickString", "description": "Mode argument for trebuchet:",                        "options": ["", "--includeCardinals"],                      "default": "--includeCardinals" },
        { "id": "cube-conundrum-mode",                  "type": "pickString", "description": "Mode argument for cube-conundrum:",                   "options": ["sumOfGameIDs", "sumOfGamePowers"],             "default": "sumOfGamePowers" },
        { "id": "gear-ratios-mode",                     "type": "pickString", "description": "Mode argument for gear-ratios:",                      "options": ["partNumberSum", "gearRatioSum"],               "default": "gearRatioSum" },
        { "id": "scratchcards-mode",                    "type": "pickString", "description": "Mode argument for scratchcards:",                     "options": ["sumOfPointsWon", "countOfScratchcardsWon"],    "default": "countOfScratchcardsWon" },
        { "id": "if-you-give-a-seed-a-fertilizer-mode", "type": "pickString", "description": "Mode argument for if-you-give-a-seed-a-fertilizer:",  "options": [],                                              "default": "" },
        { "id": "historian-hysteria-mode",              "type": "pickString", "description": "Mode argument for historian-hysteria:",               "options": ["distance", "similarity"],                      "default": "similarity" },
        { "id": "red-nosed-reports-mode",               "type": "pickString", "description": "Mode argument for red-nosed-reports:",                "options": ["strict", "tolerant"],                          "default": "tolerant" },
        { "id": "secret-entrance-mode",                 "type": "pickString", "description": "Mode argument for secret-entrance:",                  "options": ["countZeros"],                                  "default": "countZeros" },

    ],
